apiVersion: deployments.plural.sh/v1alpha1
kind: ServiceDeployment
metadata:
  name: agent-20-testing-same-wave-ordering
  namespace: testing
spec:
  namespace: testing
  repositoryRef:
    name: {{ configuration.repositoryName }}
  git:
    folder: resources/agent/20-same-wave-ordering
    ref: main
  clusterRef:
    kind: Cluster
    name: {{ configuration.cluster }}
    namespace: testing
  configuration:
    testScenario: "same-wave-ordering"
    description: "Validate behavior when multiple resources have the same wave value"


