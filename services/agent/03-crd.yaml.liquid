apiVersion: deployments.plural.sh/v1alpha1
kind: ServiceDeployment
metadata:
  name: agent-03-testing-crd
  namespace: testing
spec:
  namespace: testing
  repositoryRef:
    name: {{ configuration.repositoryName }}
  git:
    folder: resources/agent/03-crd
    ref: main
  clusterRef:
    kind: Cluster
    name: {{ configuration.cluster }}
    namespace: testing
  configuration:
  - name: testScenario
    value: "crd-mixed"
  - name: enableCustomHealth
    value: "true"
  - name: pipelineGateEnabled
    value: "true"

