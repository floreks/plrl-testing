apiVersion: deployments.plural.sh/v1alpha1
kind: ServiceDeployment
metadata:
  name: agent-01-testing-mirror
  namespace: testing
spec:
  namespace: testing
  repositoryRef:
    name: {{ configuration.repositoryName }}
    namespace: {{ configuration.repositoryNamespace }}
  git:
    folder: resources/agent/mirror
    ref: main
  clusterRef:
    kind: Cluster
    name: {{ configuration.cluster }}
    namespace: testing